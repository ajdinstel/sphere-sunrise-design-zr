<!DOCTYPE html>
<html lang="en">

<head>
  {{> common/htmlhead}}
</head>

<body>
  <div class="darkbg hidden"></div>
  {{> common/header}}
  <div class="my-orders">
    <div class="container">
      <div class="my-account-title">
        <span class="my-account-title-text icon-user">{{content.pageTitle}}</span>
      </div>
      <div class="row">
        <div class="col-sm-12">
          <div class="my-orders-wrapper">
            <div class="row">
              <div class="col-sm-3">
                <div class="my-account-sidebar">
                  <div class="my-account-sidebar-items">
                    <a href="{{content.sidebarItemOne.url}}">{{content.sidebarItemOne.text}}</a>
                  </div>
                  <div class="my-account-sidebar-items">
                    <a href="{{content.sidebarItemTwo.url}}">{{content.sidebarItemTwo.text}}</a>
                  </div>
                  <div class="my-account-sidebar-items">
                    <a href="{{content.sidebarItemThree.url}}">{{content.sidebarItemThree.text}}</a>
                  </div>
                  <div class="my-account-sidebar-items active">
                    <a href="{{content.sidebarItemFour.url}}">{{content.sidebarItemFour.text}}</a>
                  </div>
                  <div class="my-orders-order-content visible-xs">
                    <div class="your-order-details-wrapper">
                      <div class="your-order-details-title">{{content.yourOrderTitle}}</div>
                      <div class="your-order-details">
                        <span class="your-order-details-titles">{{content.orderNumberTitle}}<br>{{content.orderDateTitle}}</span>
                        <span class="your-order-details-info">{{content.orderNumber}}<br>{{content.orderDate}}</span>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-sm-6">
                        <div class="my-orders-order-dark-box">
                          <div class="my-orders-order-dark-box-title">{{content.shippingAddress.title}}</div>
                          <div>{{content.shippingAddress.name}}</div>
                          <div>{{content.shippingAddress.address}}</div>
                          <div>{{content.shippingAddress.city}}</div>
                          <div>{{content.shippingAddress.region}}</div>
                          <div>{{content.shippingAddress.postCode}}</div>
                          <div>{{content.shippingAddress.country}}</div><br>
                          <div>{{content.shippingAddress.number}}</div>
                          <div>{{content.shippingAddress.email}}</div>
                        </div>
                      </div>
                      <div class="col-sm-6">
                        <div class="my-orders-order-dark-box">
                          <div class="my-orders-order-dark-box-title">{{content.billingAddress.title}}</div>
                          <div>{{content.billingAddress.name}}</div>
                          <div>{{content.billingAddress.address}}</div>
                          <div>{{content.billingAddress.city}}</div>
                          <div>{{content.billingAddress.region}}</div>
                          <div>{{content.billingAddress.postCode}}</div>
                          <div>{{content.billingAddress.country}}</div><br>
                          <div>{{content.billingAddress.number}}</div>
                          <div>{{content.billingAddress.email}}</div>
                        </div>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-sm-6">
                        <div class="my-orders-order-dark-box">
                          <div class="my-orders-order-dark-box-title">{{content.shippingMethod.title}}</div>
                          <div>{{content.shippingMethod.text}}</div>
                        </div>
                      </div>
                      <div class="col-sm-6">
                        <div class="my-orders-order-dark-box">
                          <div class="my-orders-order-dark-box-title">{{content.paymentDetails.title}}</div>
                          <div>{{content.paymentDetails.payment}}</div>
                          <div>{{content.paymentDetails.name}}</div>
                          <div>{{content.paymentDetails.type}}</div>
                          <div>{{content.paymentDetails.number}}</div>
                          <div>{{content.paymentDetails.date}}</div>
                        </div>
                      </div>
                    </div>
                    <div class="my-orders-order-price-summary-wrapper">
                      <div class="text-right">
                        <span class="my-orders-order-promocode-title">{{content.promoCodeTitle}}</span>
                        <span class="my-orders-order-promocode">{{content.promoCode}}</span>
                      </div>
                      <div class="text-right">
                        <div class="my-orders-order-price-titles">
                          <span>{{content.subtotalTitle}}</span><br>
                          <span>{{content.orderDiscountTitle}}</span><br>
                          <span>{{content.standartDeliveryTitle}}</span>
                        </div>
                        <div class="my-orders-order-price">
                          <span>{{content.subtotal}}</span><br>
                          <span>{{content.orderDiscount}}</span><br>
                          <span>{{content.standartDelivery}}</span>
                        </div>
                        <div class="order-hr"></div>
                        <div class="my-orders-order-price-titles">
                          <span>{{content.salesTaxTitle}}</span><br>
                          <span class="order-total-big">{{content.orderTotalTitle}}</span>
                        </div>
                        <div class="my-orders-order-price">
                          <span>{{content.salesTax}}</span><br>
                          <span class="order-total-big">{{content.orderTotal}}</span>
                        </div>
                      </div>
                    </div>
                    {{#each content.order}}
                      <div class="order-detail-wrapper">
                        <div class="row">
                          <div class="col-sm-2 col-xs-5">
                            <div>
                              <img class="img-responsive" src="{{image}}" alt="Product image">
                            </div>
                          </div>
                          <div class="col-sm-4 col-xs-7">
                            <div>
                              <div class="order-detail-product-title">{{productTitleOne}}</div>
                              <div class="order-detail-product-title">{{productTitleTwo}}</div>
                              <div class="order-sku">{{sku}}</div>
                              <div class="order-detail-color-size-title">
                                <span>{{colorTitle}}</span><br>
                                <span>{{sizeTitle}}</span>
                              </div>
                              <div class="order-detail-color-size">
                                <span>{{color}}</span><br>
                                <span>{{size}}</span>
                              </div>
                              <div class="order-detail-available">{{available}}</div>
                            </div>
                          </div>
                          <div class="col-sm-2">
                            <div class="text-right">
                              {{quantity}}
                            </div>
                          </div>
                          <div class="col-sm-2">
                            <div class="text-right">
                              <span class="order-detail-discount-price">{{discountedPrice}}</span>
                              <span>{{price}}</span>
                            </div>
                          </div>
                          <div class="col-sm-2">
                            <div class="text-right">
                              {{totalPrice}}
                            </div>
                          </div>
                        </div>
                      </div>
                    {{/each}}
                  </div>
                  <div class="my-account-sidebar-items">
                    <a href="{{content.sidebarItemFive.url}}">{{content.sidebarItemFive.text}}</a>
                  </div>
                  <div class="my-account-sidebar-items clearfix">
                    <a href="{{content.sidebarItemSix.url}}">
                      <span><img class="sidebar-wishlist-heart hidden-sm" src="{{meta.assetsPath}}/img/heart-1.png" alt="heart icon"></span>{{content.sidebarItemSix.text}}<span class="sidebar-wishlist-quantity hidden-sm">{{content.sidebarItemSix.wishlistItems}}</span>
                    </a>
                  </div>
                  <div class="my-account-sidebar-items">
                    <a href="{{content.sidebarItemSeven.url}}">{{content.sidebarItemSeven.text}}</a>
                  </div>
                </div>
              </div>
              <div class="col-sm-9">
                <div class="my-orders-order-content hidden-xs">
                  <div class="row">
                    <div class="col-sm-12">
                      <div class="my-orders-title">
                        {{content.contentTitle}}
                      </div>
                    </div>
                  </div>
                  <div class="your-order-details-wrapper">
                    <div class="your-order-details-title">{{content.yourOrderTitle}}</div>
                    <div class="your-order-details">
                      <span class="your-order-details-titles">{{content.orderNumberTitle}}<br>{{content.orderDateTitle}}</span>
                      <span class="your-order-details-info">{{content.orderNumber}}<br>{{content.orderDate}}</span>
                    </div>
                    <button class="my-orders-print-receipt-btn">{{content.printReceiptBtn}}</button>
                  </div>
                  <div class="row">
                    <div class="col-sm-6">
                      <div class="my-orders-order-dark-box">
                        <div class="my-orders-order-dark-box-title">{{content.shippingAddress.title}}</div>
                        <div>{{content.shippingAddress.name}}</div>
                        <div>{{content.shippingAddress.address}}</div>
                        <div>{{content.shippingAddress.city}}</div>
                        <div>{{content.shippingAddress.region}}</div>
                        <div>{{content.shippingAddress.postCode}}</div>
                        <div>{{content.shippingAddress.country}}</div><br>
                        <div>{{content.shippingAddress.number}}</div>
                        <div>{{content.shippingAddress.email}}</div>
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="my-orders-order-dark-box">
                        <div class="my-orders-order-dark-box-title">{{content.billingAddress.title}}</div>
                        <div>{{content.billingAddress.name}}</div>
                        <div>{{content.billingAddress.address}}</div>
                        <div>{{content.billingAddress.city}}</div>
                        <div>{{content.billingAddress.region}}</div>
                        <div>{{content.billingAddress.postCode}}</div>
                        <div>{{content.billingAddress.country}}</div><br>
                        <div>{{content.billingAddress.number}}</div>
                        <div>{{content.billingAddress.email}}</div>
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-sm-6">
                      <div class="my-orders-order-dark-box">
                        <div class="my-orders-order-dark-box-title">{{content.shippingMethod.title}}</div>
                        <div>{{content.shippingMethod.text}}</div>
                      </div>
                    </div>
                    <div class="col-sm-6">
                      <div class="my-orders-order-dark-box">
                        <div class="my-orders-order-dark-box-title">{{content.paymentDetails.title}}</div>
                        <div>{{content.paymentDetails.payment}}</div>
                        <div>{{content.paymentDetails.name}}</div>
                        <div>{{content.paymentDetails.type}}</div>
                        <div>{{content.paymentDetails.number}}</div>
                        <div>{{content.paymentDetails.date}}</div>
                      </div>
                    </div>
                  </div>
                  <div class="my-orders-order-price-summary-wrapper">
                    <div class="text-right">
                      <span class="my-orders-order-promocode-title">{{content.promoCodeTitle}}</span>
                      <span class="my-orders-order-promocode">{{content.promoCode}}</span>
                    </div>
                    <div class="text-right">
                      <div class="my-orders-order-price-titles">
                        <span>{{content.subtotalTitle}}</span><br>
                        <span>{{content.orderDiscountTitle}}</span><br>
                        <span>{{content.standartDeliveryTitle}}</span>
                      </div>
                      <div class="my-orders-order-price">
                        <span>{{content.subtotal}}</span><br>
                        <span>{{content.orderDiscount}}</span><br>
                        <span>{{content.standartDelivery}}</span>
                      </div>
                      <div class="order-hr"></div>
                      <div class="my-orders-order-price-titles">
                        <span>{{content.salesTaxTitle}}</span><br>
                        <span class="order-total-big">{{content.orderTotalTitle}}</span>
                      </div>
                      <div class="my-orders-order-price">
                        <span>{{content.salesTax}}</span><br>
                        <span class="order-total-big">{{content.orderTotal}}</span>
                      </div>
                    </div>
                  </div>
                  <div class="order-list-summary-titles-wrapper">
                    <div class="row">
                      <div class="col-sm-6">
                        <div>
                          {{content.productDescriptionTitle}}
                        </div>
                      </div>
                      <div class="col-sm-2">
                        <div class="text-right">
                          {{content.quantityTitle}}
                        </div>
                      </div>
                      <div class="col-sm-2">
                        <div class="text-right">
                          {{content.priceTitle}}
                        </div>
                      </div>
                      <div class="col-sm-2">
                        <div class="text-right">
                          {{content.totalPriceTitle}}
                        </div>
                      </div>
                    </div>
                  </div>
                  {{#each content.order}}
                    <div class="order-detail-wrapper">
                      <div class="row">
                        <div class="col-sm-2">
                          <div>
                            <img class="img-responsive" src="{{image}}" alt="Product image">
                          </div>
                        </div>
                        <div class="col-sm-4">
                          <div>
                            <div class="order-detail-product-title">{{productTitleOne}}</div>
                            <div class="order-detail-product-title">{{productTitleTwo}}</div>
                            <div class="order-sku">{{sku}}</div>
                            <div class="order-detail-color-size-title">
                              <span>{{colorTitle}}</span><br>
                              <span>{{sizeTitle}}</span>
                            </div>
                            <div class="order-detail-color-size">
                              <span>{{color}}</span><br>
                              <span>{{size}}</span>
                            </div>
                            <div class="order-detail-available">{{available}}</div>
                          </div>
                        </div>
                        <div class="col-sm-2">
                          <div class="text-right">
                            {{quantity}}
                          </div>
                        </div>
                        <div class="col-sm-2">
                          <div class="text-right">
                            <span class="order-detail-discount-price">{{discountedPrice}}</span>
                            <span>{{price}}</span>
                          </div>
                        </div>
                        <div class="col-sm-2">
                          <div class="text-right">
                            {{totalPrice}}
                          </div>
                        </div>
                      </div>
                    </div>
                  {{/each}}
                  <div class="order-print-receipt-btn-wrapper">
                    <button class="order-print-receipt-btn">{{content.printReceiptBtn}}</button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  {{!-- {{> catalog/wishlist}} --}}
  {{> common/footer}}
  {{> common/htmlscripts}}
</body>

</html>
