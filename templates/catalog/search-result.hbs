<div class="search-result">
  <div class="search-results-title">{{i18n "catalog:searchResult.title"}}</div>
  <div class="search-result">{{search-result.keyword}}</div>
  <hr class="search-result-hr">
</div>
<div class="search-result-details">
  <span class="search-result-details-data">{{search-result.resultsPerPage}}</span>
  <span class="search-result-details-text">{{i18n "catalog:searchResult.of"}}</span>
  <span class="search-result-details-data">{{search-result.resultsTotal}}</span>
  <span class="search-result-details-text">{{i18n "catalog:searchResult.searchResultsFor"}}</span>
  <span class="search-result-details-data">{{search-result.keyword}}</span>
</div>
<div class="search-result-warning">{{i18n "catalog:searchResult.warning"}}</div>
